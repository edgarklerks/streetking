<div class="report-element-container backgound-darkgray">
	<div class="report-element-container-title">You traveled</div>
	<div class="report-element-container-inner">
		<div class="report-element-container-inner-left">
			<div>Continent:&nbsp;<span>[:continent_from]</span></div>
			<div>City:&nbsp;<span>[:city_from]</span></div>
		</div>
		<div class="report-element-container-inner-center">
			<div class="report-element-arrow">&rarr;</div>
		</div>
		<div class="report-element-container-inner-right">
			<div>Continent:&nbsp;<span>[:continent_to]</span></div>
			<div>City:&nbsp;<span>[:city_to]</span></div>
		</div>
		<div class="clearfix"></div>
		<div class="report-element-additional-info-box">
			<div>Time:&nbsp;<span>[:eval TIMESTAMPTODATE(time)]</span></div>
		</div>
	</div>
</div>