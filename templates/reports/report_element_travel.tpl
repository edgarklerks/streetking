<div class="small-element-container ui-corner-all">
	<div class="small-element-image-container">
		<div class="small-element-image-box sk-icons-100x100-white-tr-75 sk-icons-100x100-reporttravel">&nbsp;</div>
	</div>
	<div class="small-element-info-container">
		<div class="small-element-info-title">
			[:when (report_descriptor == "travel_report")]{You traveled}
		</div>
		<div class="small-element-info-box small-element-info-box70">
			<div class="small-element-info-about-full">
				<div>From:&nbsp;<b>[:city_from]</b> (<b>[:continent_from]</b>)</div>
				<div>To:&nbsp;<b>[:city_to]</b> (<b>[:continent_to]</b>)</div>
			</div>
		</div>
		<div class="small-element-info-additional-box">
			<div class="small-element-info-additional-left-box">
			</div>
			<div class="small-element-info-additional-right-box">
			</div>
		</div>
		<div class="small-element-additional-info-box">
			<div>Time:&nbsp;<b class="timestamp">[:time]</b></div>
		</div>
	</div>
	<div class="crearfix"></div>
</div>